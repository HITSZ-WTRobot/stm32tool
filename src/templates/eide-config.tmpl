\{
  "name": "{project_name}",
  "type": "ARM",
  "dependenceList": [],
  "srcDirs": {src_dirs | unescaped},
  "virtualFolder": \{
    "name": "<virtual_root>",
    "files": {src_files | unescaped},
    "folders": []
  },
  "outDir": "build",
  "deviceName": null,
  "packDir": null,
  "targets": \{
    "Debug": \{
      "excludeList": [],
      "toolchain": "GCC",
      "compileConfig": \{
        "cpuType": "Cortex-M4",
        "archExtensions": "",
        "floatingPointHardware": "{floating_point_hardware}",
        "scatterFilePath": "{ld_file_path}",
        "useCustomScatterFile": true,
        "storageLayout": \{
          "RAM": [],
          "ROM": []
        },
        "options": "null"
      },
      "uploader": "STLink",
      "uploadConfig": \{
        "bin": "",
        "proType": "SWD",
        "resetMode": "default",
        "runAfterProgram": true,
        "speed": 4000,
        "address": "0x08000000",
        "elFile": "None",
        "optionBytes": ".eide/debug.st.option.bytes.ini",
        "otherCmds": ""
      },
      "uploadConfigMap": \{
        "JLink": \{
          "bin": "",
          "baseAddr": "",
          "cpuInfo": \{
            "vendor": "null",
            "cpuName": "null"
          },
          "proType": 1,
          "speed": 8000,
          "otherCmds": ""
        }
      },
      "custom_dep": \{
        "name": "default",
        "incList": {include_list | unescaped},
        "libList": [],
        "defineList": {define_list | unescaped}
      },
      "builderOptions": \{
        "GCC": \{
          "version": 5,
          "beforeBuildTasks": [],
          "afterBuildTasks": [],
          "global": \{
            "$float-abi-type": "{fpu_type}",
            "output-debug-info": "enable",
            "use-newlib-nano": true,
            "not-use-syscalls": true,
            "misc-control": "-Wl,-u,_printf_float"
          },
          "c/cpp-compiler": \{
            "language-c": "c11",
            "language-cpp": "c++11",
            "optimization": "level-debug",
            "warnings": "all-warnings",
            "one-elf-section-per-function": true,
            "one-elf-section-per-data": true,
            "C_FLAGS": "",
            "CXX_FLAGS": ""
          },
          "asm-compiler": \{
            "ASM_FLAGS": ""
          },
          "linker": \{
            "$outputTaskExcludes": [
              ".bin"
            ],
            "output-format": "elf",
            "remove-unused-input-sections": true,
            "LD_FLAGS": "",
            "LIB_FLAGS": "-lm"
          }
        }
      }
    }
  },
  "version": "3.6"
}